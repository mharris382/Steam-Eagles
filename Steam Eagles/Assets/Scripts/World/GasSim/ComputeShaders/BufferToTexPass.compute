#pragma kernel CSMain

struct Cell
{
    int2 position;
    float pressure;
};

int4 bounds;
float4 globalWind;

Texture2D<float> solid;
StructuredBuffer<Cell> cells;

RWTexture2D<float> gas;


[numthreads(10,1,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    Cell cell = cells[id.x];
    gas[cell.position.xy] = cell.pressure/16.0;
}
